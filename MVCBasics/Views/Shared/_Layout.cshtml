 <!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <script src="~/lib/bootstrap/js/bootstrap.js"></script>
    <link href="~/lib/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/StyleSheet.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.1/jquery.min.js" integrity="sha512-aVKKRRi/Q/YV+4mjoKBsE4x3H+BkegoM/em46NNlCqNTmUYADjBbeNefNxYV7giUp0VxICtqdrbqU7iVaeZNXA==" crossorigin="anonymous" referrerpolicy="no-referrer" defer></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg bg-light">
            <div class="container-fluid">
                <a class="navbar-brand" asp-controller="Home" asp-action="Index">SunCode Media</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-link"><a asp-controller="Home" asp-action="Index" class="nav-link">Home</a></li>
                        <li class="nav-link"><a asp-controller="Home" asp-action="About" class="nav-link">About</a></li>
                        <li class="nav-link"><a asp-controller="Home" asp-action="Contact" class="nav-link">Contact</a></li>
                        <li class="nav-link"><a asp-controller="Home" asp-action="Projects" class="nav-link">Projects</a></li>
                        <li class="nav-link"><a asp-controller="Doctor" asp-action="CheckFever" class="nav-link">DOCTOR</a></li>
                        <li class="nav-link"><a asp-controller="GuessingGame" asp-action="Index" class="nav-link">GuessingGame</a></li>
                        <li class="nav-link"><a asp-controller="People" asp-action="Index" class="nav-link">Persons</a></li>
                        <li class="nav-link"><a asp-controller="Ajax" asp-action="Index" class="nav-link">Ajax</a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="container-fluid main-content">
        @RenderBody()
    </div>

    <footer class="container-fluid my-4">
        <span class="fw-bold">&copy; SunCode Media @DateTime.Now.Year</span>
    </footer>

</body>
</html>
